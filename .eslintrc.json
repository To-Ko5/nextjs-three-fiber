{
  "extends": ["next/core-web-vitals", "airbnb-base"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    // "no-console": ["warn"]
    "linebreak-style": "off",
    "semi": ["off"],
    "comma-dangle": ["off"],
    "no-undef": ["off"],
    "import/no-extraneous-dependencies": ["off"],
    "import/prefer-default-export": ["off"],
    "import/extensions": ["off"],
    "object-curly-newline": ["off"],
    "arrow-body-style": ["off"]
  }
}
